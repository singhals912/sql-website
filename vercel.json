{
  "buildCommand": "cd sql-practice-platform/frontend && npm install && npm run build",
  "outputDirectory": "sql-practice-platform/frontend/build",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://sql-website-production-d4d1.up.railway.app/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}